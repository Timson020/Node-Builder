{
  "name": "Node-Builder",
  "version": "1.0.0",
  "description": "快速搭建Node服务",
  "main": "index.js",
  "engines": {
    "node": ">= 8.11.3"
  },
  "scripts": {
    "init": "npm run cleanNode && babel-node ./tools/init.js",
    "dev": "cross-env NODE_ENV=development babel-node ./index.js",
    "release": "cross-env NODE_ENV=production webpack ---mode=production --progress --colors --profile --display-modules",
    "cleanNode": "rm -fr package-lock.json && rm -fr yarn.lock && rm -fr node_modules && npm i"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "cookie-parser": "1.4.3",
    "crypto-js": "3.1.9-1",
    "ejs": "2.6.1",
    "express": "4.16.3",
    "hiredis": "0.5.0",
    "moment": "2.22.2",
    "mongoose": "5.2.5",
    "node-schedule": "1.3.0",
    "redis": "2.8.0",
    "winston": "3.0.0",
    "winston-daily-rotate-file": "3.3.0"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.6",
    "babel-loader": "7.1.5",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-stage-3": "6.24.1",
    "cross-env": "5.2.0",
    "eslint": "5.1.0",
    "webpack": "4.16.1",
    "webpack-cli": "3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Timson020/Node-Builder.git"
  },
  "keywords": [
    "webpack",
    "node",
    "express",
    "builder"
  ],
  "author": "Timson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Timson020/Node-Builder/issues"
  },
  "homepage": "https://github.com/Timson020/Node-Builder#readme"
}
